{
    "name": "newmd-backend",
    "version": "1.7.4",
    "description": "An api for mingdao's potato server.",
    "main": "app.js",
    "engines": {
        "node": "18"
    },
    "type": "module",
    "scripts": {
        "dev": "nodemon --trace-warnings ./app.js",
        "start": "node ./app.js",
        "lint": " npx eslint ./",
        "check-and-fix-lint": "(echo '\n---\n\nScanning for lint error...' && eslint .) || (echo '\n\n---\n\nFound lint error, working on it...' && eslint . --fix) || echo \"\n\nWARNING: There are some error that can't be fixed automatically!\"",
        "lintfix": "npm run --silent check-and-fix-lint",
        "jest-test":"cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
        "test": "npm run --silent jest-test"
    },
    "author": "AAAA",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.4.0",
        "body-parser": "^1.20.0",
        "cheerio": "^1.0.0-rc.12",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.17.2",
        "express-rate-limit": "^6.7.0",
        "fs": "^0.0.1-security",
        "iconv-lite": "^0.6.3",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^6.11.3",
        "morgan": "^1.10.0",
        "node-fetch": "^2.6.7",
        "node-schedule": "^2.1.0"
    },
    "devDependencies": {
        "eslint": "^8.30.0",
        "nodemon": "^3.0.1"
    }
}